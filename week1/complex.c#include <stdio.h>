#include <stdio.h>struct complex{int real,img;}num[10];int main(){ int n=10,i,num_real=0,num_img=0; for(i=0;i<n;i++) {  printf("enter the real and imganiary part of a complex  number");  scanf("%d",&num[i].real);  scanf("%d",&num[i].img);  num_real=num_real+num[i].real;  num_img=num_img+num[i].img;  printf("sum of real part %d\n",num_real);  printf("sum of imaginary part %d\n",num_img);  return 0; }}
